{
  "kind": "collectionType",
  "collectionName": "tracks",
  "info": {
    "name": "tracks",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "duration": {
      "type": "biginteger",
