      "type": "integer",
      "required": true
    },
    "synchronization": {
      "via": "tracks",
      "model": "synchronizations"
    },
    "totalDistance": {
      "type": "decimal",
      "required": true
    },
    "scoreDistance": {
      "type": "decimal",
      "required": true
    },
    "envirocar": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "converted": {
      "type": "boolean",
      "default": false
    },
    "phaseNumber": {
      "type": "integer",
      "required": true
    },
    "purchase": {
